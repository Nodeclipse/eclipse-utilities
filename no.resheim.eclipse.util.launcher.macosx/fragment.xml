<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="no.resheim.eclipse.launcherutil.macosx.handlers.OpenWorkspaceHandler"
            commandId="no.resheim.eclipse.launcherutil.commands.newInstance">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="true"
            class="no.resheim.eclipse.launcherutil.macosx.handlers.OpenWorkspaceMenu"
            locationURI="menu:file?before=org.eclipse.ui.file.restartWorkbench">
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="no.resheim.eclipse.launcherutil.commands.newInstance"
            name="Open Workspace">
         <commandParameter
               id="no.resheim.eclipse.launcherutil.workspace"
               name="workspace"
               optional="true">
         </commandParameter>
      </command>
   </extension>
   <extension
         point="no.resheim.eclipse.util.launcher.core.workspace">
      <decorator
            class="no.resheim.eclipse.launcherutil.macosx.WorkspaceDecorator">
         decorator body text
      </decorator>
   </extension>

</fragment>
